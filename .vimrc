set tabstop=4
set shiftwidth=4
set smarttab
set number
set et
set wrap
set ai
set cin
set hlsearch
set incsearch
set ignorecase
set listchars=tab:⋅⋅
set list
set completeopt=menu,menuone
vmap <C-c> "+yi
vmap <C-x> "+c
vmap <C-v> c<ESC>"+p
imap <C-v> <C-r><C-o>+
nnoremap <C-z> :u<CR>
inoremap <C-z> <C-o>:u<CR>
" colorscheme slate
